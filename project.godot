;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Winter-Game"
config/version="0.0.1"
run/main_scene="res://assets/shared/scenes/main.tscn"
config/features=PackedStringArray("4.3", "C#", "Mobile")
