config/icon="res://assets/shared/graphics/icon.svg"

[autoload]

GameUtil="*res://assets/shared/scenes/GameUtil.cs"

[display]

window/size/viewport_width=375
window/size/viewport_height=812
window/size/resizable=false
window/stretch/mode="canvas_items"
window/stretch/scale_mode="integer"
window/handheld/orientation=1
window/vsync/vsync_mode=0

[dotnet]

project/assembly_name="Winter-Game"

[physics]

2d/default_linear_damp=0.0
2d/default_angular_damp=0.0

[rendering]

renderer/rendering_method="mobile"
